@model SchoolManagementSystem.Models.GradeViewModel

<div>
    <h4>Grade</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Student Name
        </dt>
        <dd class="col-sm-10">
            @Model.StudentName
        </dd>
        <dt class="col-sm-2">
            Subject Name
        </dt>
        <dd class="col-sm-10">
            @Model.SubjectName
        </dd>
        <dt class="col-sm-2">
            Value
        </dt>
        <dd class="col-sm-10">
            @Model.Value
        </dd>
        <dt class="col-sm-2">
            Status
        </dt>
        <dd class="col-sm-10">
            @Model.Status
        </dd>
        <dt class="col-sm-2">
            Date Recorded
        </dt>
        <dd class="col-sm-10">
            @Model.DateRecorded.ToString("d")
        </dd>
    </dl>
</div>
